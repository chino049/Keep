\connect ice
\timing

CREATE OR REPLACE FUNCTION err_jop() RETURNS text  AS
$$
DECLARE
  result text := '';
  good_result text := 'Good';
  bad_result text := 'Bad';

BEGIN
    ALTER table nc_versionX add column RES text;
RETURN result;
EXCEPTION WHEN OTHERS THEN
  RETURN bad_result;

END

$$ LANGUAGE 'plpgsql';

