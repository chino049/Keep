passFile="`cat /etc/passwd`"; for f in $passFile; do if [ $f ] ; then echo $f | egrep '^\w+:'; fi ; done ;
passFile="`cat /etc/passwd`"; for f in $passFile; do if [ $f ] ; then echo $f | egrep '(^\w+:)(\w+:)\1 \1\2'; fi ; done ;
                                                                                egrep '(^\w+)(:)(\w+)(:).*\1'

passFile="`cat /etc/passwd`"; for f in $passFile; do if [ $f ] ; then echo $f | sed 's|.*:[ \t]*\(/.*\)\(/.*\)/.*|\1|' ; fi; done;            


echo 'pulse:x:118:127:PulseAudio daemon,,,:/var/run/pulse:/bin/false' | egrep '(^\w+)'
echo 'pulse:x:118:127:PulseAudio daemon,,,:/var/run/pulse:/bin/false' | egrep '(^\w+)(:)(\w+)(:).*\1'

echo 'pulse:x:118:127:PulseAudio daemon,,,:/var/run/pulse:/bin/false' | sed 's|(^\w+):.*||'
echo 'pulse:x:118:127:PulseAudio daemon,,,:/var/run/pulse:/bin/false' | sed 's|(^\w+):.*|/1|'
echo 'pulse:x:118:127:PulseAudio daemon,,,:/var/run/pulse:/bin/false' | sed 's|(.*):.*|/1|'
echo 'pulse:x:118:127:PulseAudio daemon,,,:/var/run/pulse:/bin/false' | sed 's|(^\w+):\(/.*\)\(/.*\)/.*|\2|'
echo 'pulse:x:118:127:PulseAudio daemon,,,:/var/run/pulse:/bin/false' | sed 's|(^\w+)(:)(\w+)(:)|\2|'

cat /etc/passwd | sed 's|.*:[ \t]*\(/.*\)\(/.*\)/.*|\1|'

MOUNTS=`coreadm | grep "global core file pattern" 
| sed 's|.*:[ \t]*\(/.*\)\(/.*\)/.*|\1 \1\2|'`
for MOUNT in $MOUNTS; do
    echo $MOUNT
    if [ -d "$MOUNT" ]; then
        if [ `/bin/grep -c $MOUNT /etc/vfstab` -ne 0 ]; then
            /bin/echo "$MOUNT exists and is listed as a separate partition in /etc/vfstab.";
        else
            /bin/echo "$MOUNT exists and is not listed as a separate partition in /etc/vfstab.";
        fi;
     else
         /bin/echo "$MOUNT does not exist";
     fi;
done


foreach n (`seq 0 255`)
echo -n 111.222.$n.1,
end


if [ `zonename` == "global" ] ; then
  echo `zonename; svcs -Ho state ntp`
  if [ `svcs -Ho state ntp` == "disabled" ]; then
    echo "global disabled";
  elif [`svcs -Ho state ntp` == "enable" ]; then
    echo "global enable";
  fi;
else ;
  echo "non-global" ;
fi;


